---
import Head from "../components/Head.astro"
import Footer from "../components/Footer.astro"

export interface Props {
  title: string
}

const { title } = Astro.props
---

<!DOCTYPE html>
<html lang="en">
  <Head title={title} />
  <body class="bg-zinc-100 text-zinc-800 dark:bg-zinc-800 dark:text-zinc-100">
    <slot />
    <Footer />
  </body>
</html>
